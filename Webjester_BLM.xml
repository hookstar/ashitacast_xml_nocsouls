            <ear2 augment="S14968618926288334851">Gifted Earring</ear2>
            <ring1 augment="S14968618926288334851">Stikini Ring</ring1>
            <ring2 augment="S14968618926288334851">Stikini Ring</ring2>
            <back augment="S14968618926288334851">Immortal's Cape</back>
        </set>
        <include>
        </include>
    </sets>
    <init>
        <command>/macro book 5</command>
        <command delay="1000">/macro set 5</command>
        <command delay="1100">/echo [Switching to BLM Macros]</command>
        <command>/bind F9 /toggle</command>
        <command>/bind F10 /status</command>
        <command delay="2000">/lockstyleset 1</command>
    </init>
    <variables>
        <var name="toggle">0</var>     
        <var name="Weapon">Laevateinn</var>
    </variables>
    <inputcommands>
        <cmd input="/toggle">
            <if advanced="$toggle=1">
                <setvar name="Weapon" value="Laevateinn" />
                <setvar name="toggle" value="0" />
                <doidlegear />
                <command>/echo [Laevateinn]</command>
            </if>
            <else>
                <setvar name="Weapon" value="Hvergelmir" />
                <setvar name="toggle" value="1" />
                <doidlegear />
                <command>/echo [Hvergelmir]</command>
            </else>
        </cmd>
        <cmd input="/status">
            <if advanced="$toggle=0">
                <equip>
                    <back augment="S14968618926288334851">Immortal's Cape</back>
                </equip>
                <command>/echo [Laevateinn]</command>
                <doidlegear />
            </if>
            <else>
                <equip>
                    <back augment="S14968618926288334851">Immortal's Cape</back>
                </equip>
                <command>/echo [Hvergelmir]</command>
                <doidlegear />
            </else>
        </cmd>
    </inputcommands>
    <idlegear>
        <if eq_back="Nexus Cape">
            <equip lock="true">
                <back>Nexus Cape</back>
            </equip>
        </if>
        <if p_status="idle">    
            <if p_joblevel=">50&<99">
                <equip set="Idle">
                    <main>Chatoyant Staff</main>
                </equip>
            </if>
            <else>
                <if advanced="$empy=1">
                    <equip set="Idle">
                        <main>Hvergelmir</main>
                    </equip>
                </if>
                <else>
                    <equip set="Idle" />
                </else>
            </else>
        </if>
    </idlegear>
    <preranged>
    </preranged>
    <midranged>
    </midranged>
    <premagic>
        <if ad_skill="elementalmagic|enfeeblingmagic">
            <equip set="FastCast" >
                <waist />
            </equip>
        </if>
        <else>
            <equip set="FastCast" />
        </else>
    </premagic>
    <midmagic>
        <if ad_skill="elementalmagic">
            <if p_joblevel="99">
                <if advanced="(%ad_element=%e_dayelement)|(%ad_element=%e_weatherelement)">
                    <equip set="Nuke">
                        <waist augment="S7973728454464518565">Hachirin-no-Obi</waist>
                    </equip>
                </if>
                <else>
                    <equip set="Nuke" />
                </else>
            </if>
            <elseif p_joblevel=">70&<99">
                <if advanced="(%ad_element=%e_dayelement)|(%ad_element=%e_weatherelement)">
                    <equip set="Nuke71">
                        <waist augment="S7973728454464518565">Hachirin-no-Obi</waist>
                    </equip>
                </if>
                <else>
                    <equip set="Nuke71" />
                </else>
            </elseif>
            <elseif p_joblevel=">50&<71">
                <equip set="Nuke51" />
            </elseif>
            <else>
                <equip set="Nuke50" />
            </else>
        </if>
        <if ad_skill="enfeeblingmagic|bluemagic">
            <equip set="Enfeeble" />
        </if>
        <if ad_skill="darkmagic">
            <equip set="Nuke">
                <head>Appetence Crown</head>
            </equip>
        </if>
    </midmagic>
    <jobability>
    </jobability>
    <weaponskill>
    </weaponskill>
</ashitacast>
