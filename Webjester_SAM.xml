<ashitacast>
	<settings>
		<buffupdate>true</buffupdate>
		<statusupdate>true</statusupdate>
		<hpupdate>false</hpupdate>
		<autoupdate>false</autoupdate>
	</settings>
	<sets>
            <set name="idle">
                <main>$Mainhand</main>
                <sub>Utu Grip</sub>
                <range augment="S6176686717137314381">Fastwater F. Rod</range>
                <ammo augment="S6176686717137314381">Minnow</ammo>
                <head augment="S1496432389182864240">Wakido Kabuto +3</head>
                <body augment="S1496432389182864240">Wakido Domaru +3</body>
                <hands augment="S1821294470965718574">Wakido Kote +3</hands>
                <legs augment="S1496432389182864240">Wakido Haidate +3</legs>
                <feet>Danzo Sune-Ate</feet>
                <neck>Loricate Torque +1</neck>
                <waist augment="S14964323890395218574">Chaac Belt</waist>
                <ear1>Etiolation Earring</ear1>
                <ear2 augment="S182152343102764236">Stm. Earring +1</ear2>
                <ring1>Defending Ring</ring1>
                <ring2>Gelatinous Ring +1</ring2>
                <back>Agema Cape</back>
            </set>
            <set name="TP" baseset="Idle">
                <feet augment="S1821294470965718574">Wakido Sune. +3</feet>
                <neck>Asperity Necklace</neck>
                <waist>Windbuffet Belt +1</waist>
                <ear1 augment="S182109392119204240">Mache Earring +1</ear1> <!-- Augmented with TA +12% -->
                <ear2 augment="S6176686717137314381">Mache Earring +1</ear2> <!-- Augmented with DMG +32 -->
                <ring1>Regal Ring</ring1>
                <ring2>Petrov Ring</ring2>
                <back>Laic Mantle</back>
            </set>
            <set name="FastCast">
                <body augment="S6217489529050814476">Pilgrim Tunica</body>
                <ear2 augment="S182152343102764236">Stm. Earring +1</ear2>
                <ear1>Etiolation Earring</ear1>
                <ring1 augment="S6217489529050814476">Prolix Ring</ring1>
                <ring2 augment="S6217489529050814476">Ascetic's Ring</ring2>
            </set>
            <set name="MACC" baseset="Idle">
                <ammo augment="S14968618926288334851">Sinking Minnow</ammo>
                <neck augment="S14968618926288334851">Voltsurge Torque</neck>
                <waist augment="S14968618926288334851">Virtuoso Belt</waist>
                <ear1 augment="S14968618926288334851">Andoaa Earring</ear1>
                <ear2 augment="S14968618926288334851">Moldavite Earring</ear2>
                <ring1 augment="S14968618926288334851">Stikini Ring</ring1>
                <ring2 augment="S14968618926288334851">Stikini Ring</ring2>
                <back augment="S14968618926288334851">Immortal's Cape</back>
            </set>
            <set name="Meditate">
                <head augment="S1496432389182864240">Wakido Kabuto +3</head>
                <hands>Sakonji Kote +1</hands>
                <back>Smertrios's Mantle</back>
            </set>
            <set name="WS" baseset="TP">
                <neck>Fotia Gorget</neck>
                <waist>Fotia Belt</waist>
            </set>
            <set name="TH" baseset="Idle">
                <body>Volte Jupon</body>
                <hands>Volte Bracers</hands>
                <legs>Volte Hose</legs>
                <feet>Volte Boots</feet>
                <waist>Chaac Belt</waist>
            </set>
            <include>
            </include>
	</sets>
        <init>
            <command>/macro book 6</command>
            <command delay="1000">/macro set 2</command>
            <command>/echo [Switching to SAM Macros]</command>
            <command>/bind F9 /empy</command>
            <command>/bind F10 /status</command>
            <command delay="2000">/lockstyleset 24</command>
        </init>
	<variables>
            <var name="empy">1</var>
            <var name="Mainhand">Masamune</var>
	</variables>
	<inputcommands>
            <cmd input="/empy">
                <if advanced="$empy=1">
                    <setvar name="Mainhand" value="Kogarasumaru" />
                    <setvar name="empy" value="0" />
                    <doidlegear />
                    <command>/echo [Kogarasumaru Equipped]</command>
                </if>
                <else>
                    <setvar name="Mainhand" value="Masamune" />
                    <setvar name="empy" value="1" />
                    <doidlegear />
                    <command>/echo [Masamune Equipped]</command>
                </else>
            </cmd>
            <cmd input="/status">
                <if advanced="$empy=1">
                    <equip>
                        <back>Agema Cape</back>
                    </equip>
                    <command>/echo [Masamune]</command>
                    <doidlegear />
                </if>
                <else>
                    <equip>
                        <back>Agema Cape</back>
                    </equip>
                    <command>/echo [Kogarasumaru]</command>
                    <doidlegear />
                </else>
            </cmd>
	</inputcommands>
	<idlegear>
            <if eq_back="Nexus Cape">
                <equip lock="true">
                    <back>Nexus Cape</back>
                </equip>
            </if>
            <if p_status="engaged">
                <equip set="TP" />
            </if>
            <else>
                <equip set="Idle" />
            </else>
	</idlegear>
	<preranged>
	</preranged>
	<midranged>
	</midranged>
	<premagic>
            <equip set="FastCast" />
	</premagic>
	<midmagic>
            <if ad_skill="bluemagic|enfeeblingmagic|elementalmagic|darkmagic">
                <equip set="MACC" />
            </if>
	</midmagic>
	<jobability>
            <if ad_name="Meditate">
                <equip set="Meditate" />
            </if>
	</jobability>
	<weaponskill>
            <equip set="WS" />
	</weaponskill>
</ashitacast>
