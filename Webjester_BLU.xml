                <sub>$Offhand</sub>
                <range augment="S182109392119204240">Bamboo Fish. Rod</range>
                <ammo augment="S182109392119204240">Lufaise Fly</ammo>
                <head augment="S1821294470965718574">Assim. Keffiyeh +3</head>
                <body augment="S1821294470965718574">Assim. Jubbah +3</body>
                <hands augment="S1496432389182864240">Assim. Bazu. +3</hands>
