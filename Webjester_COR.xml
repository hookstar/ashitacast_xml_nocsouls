        <command>/macro book 15</command>
        <command delay="1000">/macro set 2</command>
        <command>/echo [Switching to COR Macros]</command>
        <command>/bind F9 /toggle</command>
        <command>/bind F10 /status</command>
        <command delay="4000">/lockstyleset 25</command>
    </init>
    <variables>
        <var name="toggle">0</var>
        <var name="Mainhand">Fettering Blade</var>
        <var name="Offhand">Varlet's Targe</var>
        <var name="Range">Armageddon</ranged>
        <var name="Ammo">Living Bullet</var>
    </variables>
    <inputcommands>
        <cmd input="/toggle">
            <if advanced="$toggle=1">
                <setvar name="Mainhand" value="Fettering Blade" />
                <setvar name="Offhand" value="Varlet's Targe" />
                <setvar name="Range" value="Armageddon" />
                <setvar name="toggle" value="0" />
                <doidlegear />
                <command>/echo [Archery Mode]</command>
            </if>
        </cmd>
        <cmd input="/status">
            <equip>
                <back>Agema Cape</back>
            </equip>
            <doidlegear />
            <if buffactive="4|566">
                <command>/item "Panacea" me</command>
            </if>
            <elseif buffactive="6">
                <command>/item "Echo Drops" me</command>
            </elseif>
            <elseif buffactive="9|15">
                <equip lock="true">
                    <ring2>Purity Ring</ring2>
                </equip>
                <command>/item "Hallowed Water" me</command>
            </elseif>
            <elseif buffactive="11|12|21|128|129|130|131|132|133|134|135|136|137|138|139|140|141|142|144|145|146|147|148|149|174|175|404|557|558|559|560|561|562|563|564|567">
                <command>/item "Panacea" me</command>
            </elseif>
        </cmd>
    </inputcommands>
    <idlegear>
        <if eq_back="Nexus Cape">
            <equip lock="true">
                <back>Nexus Cape</back>
            </equip>
        </if>
        <if p_status="engaged">
            <equip set="TP" />
        </if>
        <else>
            <equip set="Idle" />
        </else>
    </idlegear>
    <preranged>
        <equip set="TP" />
    </preranged>
    <midranged>
        <equip set="PhysWS" />
    </midranged>
    <premagic>
        <equip set="FastCast" />
    </premagic>
    <midmagic>
        <if ad_skill="bluemagic|enfeeblingmagic|elementalmagic|darkmagic">
            <equip set="MACC" />
        </if>
        <if ad_name="Stoneskin|Metallic*|Diamondhide">
            <if buffactive="37">
                <command>/cancel 37</command>
            </if>
        </if>
        <if ad_name="Shock Spikes">
            <if buffactive="38">
                <command>/cancel 38</command>
            </if>
        </if>
    </midmagic>
    <jobability>
    </jobability>
    <weaponskill>
        <if ad_name="Wildfire|Trueflight|Leaden Salute">
            <equip set="MagWS" />
        </if>
        <elseif ad_name="Savage*|Requies*|Death*">
            <equip set="TP">
                <neck>Fotia Gorget</neck> <!-- TA +12 -->
