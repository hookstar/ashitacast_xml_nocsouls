<ashitacast>
    <settings>
        <buffupdate>true</buffupdate>
        <statusupdate>true</statusupdate>
        <hpupdate>false</hpupdate>
        <autoupdate>false</autoupdate>
    </settings>
    <sets>
        <set name="Idle">
            <main>$Mainhand</main>
            <sub>$Offhand</sub>
            <range>$Range</range>
            <ammo>$Ammo</ammo>
            <head>Orion Beret +3</head>
            <body>Orion Jerkin +3</body>
            <hands>Orion Bracers +3</hands>
            <legs>Blood Cuisses</legs>
            <feet>Orion Socks +3</feet>
            <neck>Loricate Torque +1</neck> <!-- Counter +40 -->
            <waist>Goading Belt</waist> <!-- mEVA +40 -->
            <ear1>Etiolation Earring</ear1> <!-- Counter +40 -->
            <ear2>Cryptic Earring</ear2> <!-- mEVA +40 -->
            <ring1>Defending Ring</ring1> <!-- STP 10, TA +9 -->
            <ring2>Gelatinous Ring +1</ring2> <!-- STP 10, TA +9 -->
            <back>Agema Cape</back> <!-- TA +12 -->
        </set>
        <set name="TP" baseset="Idle">
            <head augment="S182109392262544240">Volte Tiara</head>
            <body augment="S182109392262544240">Volte Harness</body>
            <hands augment="S182109392262544240">Volte Mittens</hands>
            <legs augment="S182109392262544240">Volte Tights</legs>
            <feet augment="S182109392262544240">Volte Spats</feet>
            <neck>Tlamiztli Collar</neck>
            <waist>Windbuffet Belt +1</waist>
            <ear1 augment="S353610053140898233">Telos Earring</ear1>
            <ear2 augment="S14964323893455334841">Tati Earring</ear2>
            <ring1>Regal Ring</ring1>
            <ring2>Rajas Ring</ring2>
            <back>Laic Mantle</back>
        </set>
        <set name="RangeWS" baseset="Idle">
            <legs>Orion Braccae +3</legs>
            <neck>Gaudryi Necklace</neck>
            <waist>Ponente Sash</waist>
            <ear1 augment="S353610053140898233">Telos Earring</ear1>
            <ear2>Enervating Earring</ear2>
            <ring1>Regal Ring</ring1>
            <ring2>Longshot Ring</ring2>
            <back>Terebellum Mantle</back>
        </set>
        <set name="WSFTP" baseset="RangeWS">
            <neck>Fotia Gorget</neck> <!-- TA +12 -->
            <waist>Fotia Belt</waist> <!-- TA +12 -->
        </set>
        <set name="MagicWS" baseset="Idle">
            <body augment="S7973728454464518565">Gyve Doublet</body>
            <legs augment="S7973728454464518565">Gyve Trousers</legs>
            <head>Volte Cap</head>
            <hands>Volte Bracers</hands>
            <feet>Volte Boots</feet>
            <neck>Baetyl Pendant</neck>
            <!--head augment="S14968618926288334851">Orion Beret +2</head>
            <hands augment="S14968618926288334851">Orion Bracers +2</hands>
            <feet>Orion Socks +2</feet>
            <neck augment="S14968618926288334851">Voltsurge Torque</neck-->
            <waist augment="S7973728454464518565">Eschan Stone</waist>
            <ear1 augment="S7973728454464518565">Choleric Earring</ear1>
            <ear2 augment="S7973728454464518565">Tranquility Earring</ear2>
            <ring1 augment="S7973728454464518565">Stikini Ring +1</ring1>
            <ring2 augment="S7973728454464518565">Stikini Ring +1</ring2>
            <back augment="S7973728454464518565">Izdubar Mantle</back>
        </set>
        <set name="FastCast">
            <body augment="S6217489529050814476">Pilgrim Tunica</body>
            <ear2 augment="S182152343102764236">Stm. Earring +1</ear2>
            <ear1>Etiolation Earring</ear1>
            <ring2 augment="S6217489529050814476">Ascetic's Ring</ring2>
        </set>
        <set name="MACC" baseset="Idle">
            <neck augment="S14968618926288334851">Voltsurge Torque</neck>
            <waist augment="S14968618926288334851">Virtuoso Belt</waist>
            <ear1 augment="S14968618926288334851">Andoaa Earring</ear1>
            <ear2 augment="S14968618926288334851">Moldavite Earring</ear2>
            <ring1 augment="S14968618926288334851">Stikini Ring</ring1>
            <ring2 augment="S14968618926288334851">Stikini Ring</ring2>
            <back augment="S14968618926288334851">Immortal's Cape</back>
        </set>
        <set name="TH" baseset="Idle">
            <head>Volte Cap</head>
            <body>Volte Jupon</body>
            <hands>Volte Bracers</hands>
            <legs>Volte Hose</legs>
            <feet>Volte Boots</feet>
            <waist>Chaac Belt</waist>
        </set>
        <include>
        </include>
    </sets>
    <init>
        <command>/macro book 10</command>
        <command delay="1000">/macro set 2</command>
        <command>/echo [Switching to RNG Macros]</command>
        <command>/bind F9 /toggle</command>
        <command>/bind F10 /status</command>
        <command delay="2000">/lockstyleset 21</command>
    </init>
    <variables>
        <var name="toggle">0</var>
        <var name="Mainhand">Perun +1</var>
        <var name="Offhand">Nusku Shield</var>
        <var name="Range">Gandiva</ranged>
        <var name="Ammo">Artemis's Arrow</var>
    </variables>
    <inputcommands>
        <cmd input="/toggle">
            <if advanced="$toggle=1">
                <setvar name="Mainhand" value="Perun +1" />
                <setvar name="Offhand" value="Nusku Shield" />
                <setvar name="Range" value="Gandiva" />
                <setvar name="Ammo" value="Artemis's Arrow" />
                <setvar name="toggle" value="0" />
                <doidlegear />
                <command delay="2000">/lockstyleset 21</command>
            </if>
            <else>
                <setvar name="Mainhand" value="Blurred Axe" />
                <setvar name="Offhand" value="Varlet's Targe" />
                <setvar name="Range" value="Armageddon" />
                <setvar name="Ammo" value="Devastating Bullet" />
                <setvar name="toggle" value="1" />
                <doidlegear />
                <command delay="2000">/lockstyleset 21</command>
            </else>
            <command>/echo [$Range]</command>
        </cmd>
        <cmd input="/status">
            <doidlegear />
            <command>/echo [$Range]</command>
            <if buffactive="4|566">
                <command>/item "Panacea" me</command>
            </if>
            <elseif buffactive="6">
                <command>/item "Echo Drops" me</command>
            </elseif>
            <elseif buffactive="9|15">
                <equip lock="true">
                    <ring2>Purity Ring</ring2>
                </equip>
                <command>/item "Hallowed Water" me</command>
            </elseif>
            <elseif buffactive="11|12|21|128|129|130|131|132|133|134|135|136|137|138|139|140|141|142|144|145|146|147|148|149|174|175|404|557|558|559|560|561|562|563|564|567">
                <command>/item "Panacea" me</command>
            </elseif>
        </cmd>
    </inputcommands>
    <idlegear>
        <if eq_back="Nexus Cape">
            <equip lock="true">
                <back>Nexus Cape</back>
            </equip>
        </if>
        <if p_status="engaged">
            <equip set="TP" />
        </if>
        <else>
            <equip set="Idle" />
        </else>
    </idlegear>
    <preranged>
        <equip set="TP" />
    </preranged>
    <midranged>
        <equip set="PhysWS" />
    </midranged>
    <premagic>
        <equip set="FastCast" />
    </premagic>
    <midmagic>
        <if ad_skill="bluemagic|enfeeblingmagic|elementalmagic|darkmagic">
            <equip set="MACC" />
        </if>
        <if ad_name="Stoneskin|Metallic*|Diamondhide">
            <if buffactive="37">
                <command>/cancel 37</command>
            </if>
        </if>
        <if ad_name="Shock Spikes">
            <if buffactive="38">
                <command>/cancel 38</command>
            </if>
        </if>
    </midmagic>
    <jobability>
        <if ad_name="Camouflage">
            <equip body="Orion Jerkin +3" />
        </if>
        <if ad_name="Shadowbind">
            <equip hands="Orion Bracers +3" />
        </if>
        <if ad_name="Scavenge">
            <equip feet="Orion Socks +3" />
        </if>
    </jobability>
    <weaponskill>
        <if ad_name="Wildfire|Trueflight|Leaden Salute">
            <equip set="MagicWS" />
        </if>
        <elseif ad_name="Jishnu*">
            <equip set="WSFTP" />
        </elseif>
        <else>
            <equip set="RangeWS" />
        </else>
    </weaponskill>
</ashitacast>
