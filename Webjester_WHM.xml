<ashitacast>
	<settings>
		<buffupdate>true</buffupdate>
		<statusupdate>true</statusupdate>
		<hpupdate>false</hpupdate>
		<autoupdate>false</autoupdate>
	</settings>
	<sets>
            <set name="idle">
                <main>$Mainhand</main>
                <sub augment="S14964323880347918574">Genmei Shield</sub>
                <range augment="S7973728454464518565">Tarutaru F. Rod</range>
                <ammo augment="S182109392119204240">Lufaise Fly</ammo>
                <head>Theophany Cap +2</head>
                <body>Theo. Briault +2</body>
                <hands>Theophany Mitts +2</hands>
                <legs>Th. Pantaloons +2</legs>
                <feet>Iaso Boots</feet>
                <neck>Loricate Torque +1</neck>
                <waist augment="S14964323890395218574">Chaac Belt</waist>
                <ear1>Etiolation Earring</ear1>
                <ear2 augment="S182152343102764236">Stm. Earring +1</ear2>
                <ring1>Defending Ring</ring1>
                <ring2>Gelatinous Ring +1</ring2>
                <back>Kumbira Cape</back>
            </set>
            <set name="TP" baseset="idle">
                <range augment="S7973728454464518565">Tarutaru F. Rod</range>
                <ammo augment="S182109392119204240">Lufaise Fly</ammo>
                <feet>Theo. Duckbills +2</feet>
                <neck>Asperity Necklace</neck>
                <waist augment="S1496432389182864240">Windbuffet Belt +1</waist>
                <ear1 augment="S1496432389345534240">Mache Earring +1</ear1>
                <ear2 augment="S182109392119204240">Mache Earring +1</ear2>
                <ring1>Rajas Ring</ring1>
                <ring2>Petrov Ring</ring2>
            </set>
            <set name="FastCast">
                <body augment="S6217489529050814476">Pilgrim Tunica</body>
                <ear2 augment="S182152343102764236">Stm. Earring +1</ear2>
                <ear1>Etiolation Earring</ear1>
                <ring1 augment="S6217489529050814476">Prolix Ring</ring1>
                <ring2 augment="S6217489529050814476">Ascetic's Ring</ring2>
            </set>
            <set name="MACC" baseset="Idle">
                <ammo augment="S14968618926288334851">Sinking Minnow</ammo>
                <neck augment="S14968618926288334851">Voltsurge Torque</neck>
                <waist augment="S14968618926288334851">Virtuoso Belt</waist>
                <ear1 augment="S14968618926288334851">Andoaa Earring</ear1>
                <ear2 augment="S14968618926288334851">Moldavite Earring</ear2>
                <ring1 augment="S14968618926288334851">Stikini Ring</ring1>
                <ring2 augment="S14968618926288334851">Stikini Ring</ring2>
                <back augment="S14968618926288334851">Immortal's Cape</back>
            </set>
            <set name="MagWS" baseset="Idle">
                <ammo augment="S7973728454464518565">Sabiki Rig</ammo>
                <neck augment="S7973728454464518565">Baetyl Pendant</neck>
                <waist augment="S7973728454464518565">Hachirin-no-Obi</waist>
                <ear1 augment="S7973728454464518565">Choleric Earring</ear1>
                <ear2 augment="S7973728454464518565">Regal Earring</ear2>
                <ring1 augment="S7973728454464518565">Stikini Ring +1</ring1>
                <ring2 augment="S7973728454464518565">Stikini Ring +1</ring2>
                <back augment="S7973728454464518565">Izdubar Mantle</back>
            </set>

            <include>
            </include>
	</sets>
        <init>
            <command>/macro book 3</command>
            <command delay="1000">/macro set 2</command>
            <command>/echo [Switching to WHM Macros]</command>
            <command>/bind F9 /switch</command>
            <command>/bind F10 /status</command>
            <command delay="2000">/lockstyle on</command>
        </init>
	<variables>
            <var name="switch">1</var>
            <var name="Mainhand">Yagrush</var>
	</variables>
	<inputcommands>
            <cmd input="/switch">
                <if advanced="$switch=1">
                    <setvar name="Mainhand" value="Blurred Rod" />
                    <setvar name="switch" value="0" />
                    <command delay="1000">/macro set 4</command>
                    <doidlegear />
                    <command>/echo [Blurred Rod Equipped]</command>
                </if>
                <else>
                    <setvar name="Mainhand" value="Yagrush" />
                    <setvar name="switch" value="1" />
                    <command delay="1000">/macro set 5</command>
                    <doidlegear />
                    <command>/echo [Yagrush Equipped]</command>
                </else>
            </cmd>
            <cmd input="/status">
                <if advanced="$switch=1">
                    <equip>
                        <back>Kumbira Cape</back>
                    </equip>
                    <command>/echo [Yagrush]</command>
                    <doidlegear />
                </if>
                <else>
                    <equip>
                        <back>Kumbira Cape</back>
                    </equip>
                    <command>/echo [Blurred Rod]</command>
                    <doidlegear />
                </else>
            </cmd>
	</inputcommands>
	<idlegear>
            <if eq_back="Nexus Cape">
                <equip lock="true">
                    <back>Nexus Cape</back>
                </equip>
            </if>
            <if p_status="engaged">
                <equip set="TP" />
            </if>
            <else>
                <equip set="Idle" />
            </else>
	</idlegear>
	<preranged>
	</preranged>
	<midranged>
	</midranged>
	<premagic>
            <equip set="FastCast" />
	</premagic>
	<midmagic>
            <if ad_skill="bluemagic|enfeeblingmagic|elementalmagic|darkmagic">
                <equip set="MACC" />
            </if>
	</midmagic>
	<jobability>
	</jobability>
	<weaponskill>
            <if ad_name="Mystic*|Flash*">
                <equip set="MagWS" />
            </if>
            <else>
                <equip set="TP">
                    <neck>Fotia Gorget</neck>
                    <waist>Fotia Belt</waist>
                </equip>
            </else>
	</weaponskill>
</ashitacast>
