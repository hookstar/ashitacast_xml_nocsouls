        <command>/macro book 4</command>
        <command delay="1000">/macro set 4</command>
        <command>/echo [Switching to PUP Macros]</command>
        <command>/bind F9 /switch</command>
        <command>/bind F10 /status</command>
        <command>/bind F12 /main</command>
        <doidlegear />
        <command delay="4000">/lockstyle on</command>
        <!-- Valoredge
            Atutuner
            Flameholder
            Magniplug
            Magniplug II
            Inhibitor
            Armor Plate IV
            Shock Absorber II
            Coiler II
            Optic Fiber
            Optic Fiber II
            Flashbulb
            Manna Jammer II
        -->
        <!-- Soulsoother
            Stealth Screen II
            Optic Fiber II
            Vivi-valve II
            Damage Gauge
            Heatsink
            Mana Jammer II
            Mana Converter
            Mana Conserver
            Armor Plate IV
            Accelerator III
            Tactical Processor
            Mana Booster
        -->
    </init>
    <variables>
        <var name="switch">0</var>
        <var name="weapon">1</var>
        <var name="main">Kenkonken</var>
        <var name="ranged">Animator P</var>
    </variables>
    <inputcommands>
        <cmd input="/switch">
            <if advanced="$switch=1">
                <setvar name="ranged" value="Animator P" />
                <setvar name="switch" value="0" />
                <doidlegear />
            </if>
            <else>
                <setvar name="ranged" value="Animator P II" />
                <setvar name="switch" value="1" />
                <doidlegear />
            </else>
            <command>/echo [$main] [$ranged]</command>
        </cmd>
        <cmd input="/main">
            <if advanced="$weapon=1">
                <setvar name="main" value="Verethragna" />
                <setvar name="weapon" value="0" />
                <doidlegear />
            </if>
            <else>
                <setvar name="main" value="Kenkonken" />
                <setvar name="weapon" value="1" />
                <doidlegear />
            </else>
            <command>/echo [$main] [$ranged]</command>
        </cmd>
        <cmd input="/status">
            <equip>
                <back>Dispersal Mantle</back>
            </equip>
            <doidlegear />
            <command>/echo [$main] [$ranged]</command>
            <if buffactive="4|566">
                <command>/item "Panacea" me</command>
            </if>
            <elseif buffactive="6">
                <command>/item "Echo Drops" me</command>
            </elseif>
            <elseif buffactive="9|15">
                <equip lock="true">
                    <ring2>Purity Ring</ring2>
                </equip>
                <command>/item "Hallowed Water" me</command>
            </elseif>
            <elseif buffactive="11|12|21|128|129|130|131|132|133|134|135|136|137|138|139|140|141|142|144|145|146|147|148|149|174|175|404|557|558|559|560|561|562|563|564|567">
                <command>/item "Panacea" me</command>
            </elseif>
        </cmd>
    </inputcommands>
    <idlegear>
        <if eq_back="Nexus Cape">
            <equip lock="true">
                <back>Nexus Cape</back>
            </equip>
        </if>
        <if p_status="engaged">
            <if p_joblevel="99">
                <equip set="TP" />
            </if>
            <else>
                <equip set="TP50" />
            </else>
        </if>
        <else>
            <equip set="Idle" />
        </else>
    </idlegear>
    <preranged>
    </preranged>
    <midranged>
    </midranged>
    <premagic>
        <equip set="FastCast" />
    </premagic>
    <midmagic>
        <if ad_skill="bluemagic|enfeeblingmagic|elementalmagic|darkmagic">
            <equip set="MACC" />
        </if>
        <if ad_skill="divinemagic">
            <if ad_name="Banish*|Holy*">
                <equip set="MagWS" />
            </if>
            <else>
                <equip set="MACC" />
            </else>
        </if>
        <if ad_name="Stoneskin|Metallic*|Diamondhide">
            <if buffactive="37">
                <command>/cancel 37</command>
            </if>
        </if>
        <if ad_name="Shock Spikes">
            <if buffactive="38">
                <command>/cancel 38</command>
            </if>
        </if>
    </midmagic>
    <jobability>
        <if ad_name="Activate|Deus*">
            <equip>
                <range>$ranged</range>
                <back>Visucius's Mantle</back>
            </equip>
        </if>
    </jobability>
    <weaponskill>
        <if p_joblevel="99">
            <if ad_name="Victory*|Stringing*|Ascetic*">
                <equip set="WS">
                    <ear1>Telos Earring</ear1>
                </equip>
            </if>
            <else>
                <equip set="WS" />
            </else>
        </if>
        <else>
            <equip set="Idle" />
        </else>
    </weaponskill>
</ashitacast>
